  },
  "devDependencies": {
    "@types/node": "^6.0.52",
    "@types/tape": "^4.2.28",
    "rxjs": "^5.0.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3",
    "typescript": "^2.1.4",
    "watch": "^1.0.1"
  }
}
